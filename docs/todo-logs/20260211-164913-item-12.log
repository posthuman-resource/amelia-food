mojiComposer — A composition tray, like arranging fridge magnets */\n\n.composer {\n  display: flex;\n  align-items: center;\n  gap: var(--space-sm);\n  padding: var(--space-sm) var(--space-md);\n  background: var(--color-surface-warm);\n  border-radius: var(--radius-md);\n  border: 1px solid rgba(44, 40, 37, 0.06);\n  flex-shrink: 0;\n}\n\n/* ========================================\n   Tray — where selected emoji accumulate\n   ======================================== */\n\n.tray {\n  display: flex;\n  flex-wrap: wrap;\n  align-items: center;\n  gap: 4px;\n  flex: 1;\n  min-height: 2.5rem;\n  padding: var(--space-xs) 0;\n}\n\n/* Placeholder hint when empty */\n.hint {\n  font-family: var(--font-hand);\n  font-size: 1.1rem;\n  color: var(--color-text-muted);\n  opacity: 0.5;\n  user-select: none;\n}\n\n/* ========================================\n   Individual emoji tile — tappable to remove\n   ======================================== */\n\n.tile {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 1.5rem;\n  width: 2.25rem;\n  height: 2.25rem;\n  border-radius: var(--radius-sm);\n  cursor: pointer;\n  -webkit-tap-highlight-color: transparent;\n  -webkit-user-select: none;\n  user-select: none;\n  transition:\n    background var(--transition-fast),\n    transform var(--transition-fast),\n    opacity var(--transition-fast);\n\n  /* Entry animation — like placing a tile gently */\n  animation: tilePlaced 250ms ease-out;\n}\n\n@media (hover: hover) {\n  .tile:hover {\n    background: rgba(184, 92, 56, 0.1);\n    transform: scale(1.1);\n  }\n}\n\n.tile:active {\n  transform: scale(0.9);\n  opacity: 0.6;\n}\n\n@keyframes tilePlaced {\n  0% {\n    transform: scale(0.7);\n    opacity: 0;\n  }\n  60% {\n    transform: scale(1.08);\n    opacity: 1;\n  }\n  100% {\n    transform: scale(1);\n    opacity: 1;\n  }\n}\n\n/* ========================================\n   Action buttons\n   ======================================== */\n\n.actions {\n  display: flex;\n  align-items: center;\n  gap: var(--space-xs);\n  flex-shrink: 0;\n}\n\n/* Clear button — subtle, text-only */\n.clearBtn {\n  font-family: var(--font-hand);\n  font-size: 1rem;\n  color: var(--color-text-muted);\n  padding: var(--space-xs) var(--space-sm);\n  border-radius: var(--radius-sm);\n  -webkit-tap-highlight-color: transparent;\n  transition:\n    color var(--transition-fast),\n    background var(--transition-fast),\n    opacity var(--transition-fast);\n}\n\n@media (hover: hover) {\n  .clearBtn:hover:not(:disabled) {\n    color: var(--color-text);\n    background: rgba(44, 40, 37, 0.06);\n  }\n}\n\n.clearBtn:active:not(:disabled) {\n  background: rgba(44, 40, 37, 0.10);\n}\n\n.clearBtn:disabled {\n  opacity: 0.3;\n  cursor: default;\n}\n\n/* Send button — plum, warm, inviting */\n.sendBtn {\n  font-family: var(--font-hand);\n  font-size: 1.15rem;\n  color: var(--color-surface-light);\n  background: var(--color-plum);\n  padding: var(--space-xs) var(--space-lg);\n  border-radius: var(--radius-md);\n  -webkit-tap-highlight-color: transparent;\n  transition:\n    background var(--transition-fast),\n    opacity var(--transition-fast),\n    transform var(--transition-fast);\n}\n\n@media (hover: hover) {\n  .sendBtn:hover:not(:disabled) {\n    background: var(--color-plum-light);\n    transform: translateY(-1px);\n  }\n}\n\n.sendBtn:active:not(:disabled) {\n  transform: scale(0.97);\n}\n\n.sendBtn:disabled {\n  opacity: 0.35;\n  cursor: default;\n}\n\n/* ========================================\n   Mobile — thumb-friendly, one-hand use\n   ======================================== */\n\n@media (max-width: 768px) {\n  .composer {\n    padding: var(--space-sm);\n    gap: var(--space-sm);\n  }\n\n  .tray {\n    min-height: 2.75rem;\n  }\n\n  .tile {\n    font-size: 1.4rem;\n    width: 2.25rem;\n    height: 2.25rem;\n  }\n\n  .hint {\n    font-size: 1rem;\n  }\n\n  /* Larger send button for easy thumb tapping */\n  .sendBtn {\n    padding: var(--space-sm) var(--space-lg);\n    font-size: 1.1rem;\n    min-height: 44px;\n    min-width: 64px;\n  }\n\n  .clearBtn {\n    font-size: 0.95rem;\n    min-height: 44px;\n    padding: var(--space-sm);\n  }\n}\n","numLines":199,"startLine":1,"totalLines":199}}}
