: flex;\n  flex-direction: column;\n  gap: var(--space-sm);\n  min-height: 0;\n}\n\n/* ========================================\n   Search input — paper-textured field\n   ======================================== */\n\n.searchWrap {\n  position: relative;\n  flex-shrink: 0;\n}\n\n.search {\n  width: 100%;\n  padding: var(--space-sm) var(--space-md);\n  padding-right: 2.5rem;\n  font-family: var(--font-body);\n  font-size: 1rem;\n  color: var(--color-text);\n  background: var(--color-surface);\n  border: 1.5px solid rgba(44, 40, 37, 0.12);\n  border-radius: var(--radius-md);\n  outline: none;\n  -webkit-appearance: none;\n  transition:\n    border-color var(--transition-fast),\n    box-shadow var(--transition-fast);\n}\n\n.search::placeholder {\n  color: var(--color-text-muted);\n  font-family: var(--font-hand);\n  font-size: 1.05rem;\n  opacity: 0.65;\n}\n\n.search:focus {\n  border-color: var(--color-plum);\n  box-shadow: 0 0 0 3px rgba(107, 45, 91, 0.08);\n}\n\n.clearBtn {\n  position: absolute;\n  right: var(--space-sm);\n  top: 50%;\n  transform: translateY(-50%);\n  width: 28px;\n  height: 28px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: var(--radius-sm);\n  color: var(--color-text-muted);\n  font-size: 1.1rem;\n  line-height: 1;\n  -webkit-tap-highlight-color: transparent;\n  transition: color var(--transition-fast), background var(--transition-fast);\n}\n\n@media (hover: hover) {\n  .clearBtn:hover {\n    color: var(--color-text);\n    background: rgba(44, 40, 37, 0.06);\n  }\n}\n\n.clearBtn:active {\n  color: var(--color-text);\n  background: rgba(44, 40, 37, 0.10);\n}\n\n/* ========================================\n   Grid area — scrollable tray of tiles\n   ======================================== */\n\n.grid {\n  overflow-y: auto;\n  max-height: 320px;\n  min-height: 0;\n  flex: 1;\n  padding: var(--space-xs);\n  -webkit-overflow-scrolling: touch;\n\n  scrollbar-width: thin;\n  scrollbar-color: rgba(44, 40, 37, 0.15) transparent;\n}\n\n.grid::-webkit-scrollbar {\n  width: 4px;\n}\n\n.grid::-webkit-scrollbar-track {\n  background: transparent;\n}\n\n.grid::-webkit-scrollbar-thumb {\n  background: rgba(44, 40, 37, 0.15);\n  border-radius: 2px;\n}\n\n/* Category section */\n.section {\n  margin-bottom: var(--space-md);\n}\n\n.section:last-child {\n  margin-bottom: 0;\n}\n\n/* Category header — serif, subtle */\n.categoryHeader {\n  font-family: var(--font-serif);\n  font-size: 0.8rem;\n  font-weight: 600;\n  color: var(--color-text-muted);\n  padding: var(--space-xs) var(--space-xs);\n  margin-bottom: var(--space-xs);\n  letter-spacing: 0.02em;\n  border-bottom: 1px solid rgba(44, 40, 37, 0.06);\n}\n\n/* Tile grid — like a tray of letter tiles */\n.tiles {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(44px, 1fr));\n  gap: 2px;\n}\n\n/* Individual emoji cell — a tappable tile */\n.cell {\n  aspect-ratio: 1;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 1.5rem;\n  border-radius: var(--radius-sm);\n  -webkit-tap-highlight-color: transparent;\n  -webkit-user-select: none;\n  user-select: none;\n  cursor: pointer;\n  transition:\n    background var(--transition-fast),\n    transform var(--transition-fast);\n}\n\n/* Desktop: hover scale */\n@media (hover: hover) {\n  .cell:hover {\n    background: rgba(107, 45, 91, 0.06);\n    transform: scale(1.15);\n  }\n}\n\n/* Touch and click: press feedback */\n.cell:active {\n  transform: scale(0.92);\n  background: rgba(107, 45, 91, 0.10);\n}\n\n/* Selection flash — brief warm glow */\n.pop {\n  animation: popFlash 300ms ease-out;\n}\n\n@keyframes popFlash {\n  0% {\n    background: rgba(196, 114, 58, 0.25);\n    transform: scale(1.2);\n  }\n  100% {\n    background: transparent;\n    transform: scale(1);\n  }\n}\n\n/* ========================================\n   No results\n   ======================================== */\n\n.noResults {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: var(--space-2xl) 0;\n  gap: var(--space-sm);\n  opacity: 0.5;\n}\n\n.noResultsEmoji {\n  font-size: 2rem;\n}\n\n.noResultsText {\n  font-family: var(--font-hand);\n  font-size: 1.1rem;\n  color: var(--color-text-muted);\n  margin: 0;\n}\n\n/* ========================================\n   Mobile — thumb-friendly tiles\n   ======================================== */\n\n@media (max-width: 768px) {\n  .grid {\n    max-height: none;\n    flex: 1;\n  }\n\n  /* Ensure min 44px for thumb tapping on mobile */\n  .tiles {\n    grid-template-columns: repeat(auto-fill, minmax(44px, 1fr));\n    gap: 2px;\n  }\n\n  .cell {\n    font-size: 1.4rem;\n    /* Min touch target size */\n    min-width: 44px;\n    min-height: 44px;\n  }\n\n  .search {\n    font-size: 1rem;\n    padding: var(--space-sm) var(--space-md);\n    /* Prevent iOS zoom on focus */\n    font-size: 16px;\n  }\n\n  .clearBtn {\n    width: 36px;\n    height: 36px;\n  }\n}\n","structuredPatch":[{"oldStart":146,"oldLines":9,"newStart":146,"newLines":18,"lines":["   cursor: pointer;","   transition:","     background var(--transition-fast),","-    transform var(--transition-fast);","+    transform var(--transition-fast),","+    box-shadow var(--transition-fast);"," }"," ","+/* Focus-visible: subtle warm ring for keyboard navigation */","+.cell:focus-visible {","+  background: rgba(107, 45, 91, 0.06);","+  box-shadow: 0 0 0 2px rgba(107, 45, 91, 0.25);","+  outline: none;","+  transform: scale(1.1);","+}","+"," /* Desktop: hover scale */"," @media (hover: hover) {","   .cell:hover {"]}],"userModified":false,"replaceAll":false}}
